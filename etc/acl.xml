<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                  <resource id="DCKAP_Contactform::Contactform_main_menu">
                    <resource id="DCKAP_Contactform::Contactform_addform" title="Contact's Manager" sortOrder="10" >
                        <resource id="DCKAP_Contactform::Contactform_list" title="Manage Contact's" sortOrder="40">
                            <resource id="DCKAP_Contactform::save" title="Save Link" sortOrder="10" />
                            <resource id="DCKAP_Contactform::Contactform_delete" title="Delete Contact" sortOrder="20" />
                        </resource>
                       
                    </resource>
                </resource>
                     <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <!-- this resource id we can use in system.xml for section -->
                            <resource id="DCKAP_Contactform::config_Contactform" title="Contact Us Module Configuration" sortOrder="80" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
    
   